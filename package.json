{
  "name": "latex.ts",
  "description": "TypeScript LaTeX to HTML5 translator",
  "version": "0.0.1",
  "author": {
    "name": "RogueAI",
    "email": "rogueai@users.noreply.github.com"
  },
  "keywords": [
    "peggy",
    "latex",
    "parser",
    "html5"
  ],
  "bin": {
    "latex.js": "./bin/latex.js"
  },
  "module": "./dist/latex.mjs",
  "main": "./dist/latex.js",
  "exports": {
    "import": "./dist/latex.mjs",
    "require": "./dist/latex.js"
  },
  "browser": "./dist/latex.js",
  "files": [
    "bin/latex.js",
    "dist/latex.js",
    "dist/latex.js.map",
    "dist/latex.mjs",
    "dist/latex.mjs.map",
    "dist/css/",
    "dist/fonts/",
    "dist/js/",
    "dist/packages/",
    "dist/documentclasses/"
  ],
  "scripts": {
    "clean": "rimraf dist bin",
    "devbuild": "rimraf 'dist/**/*.map';mkdirp dist/css;mkdirp dist/js;mkdirp dist/fonts;mkdirp dist/documentclasses;mkdirp dist/packages;rsync -a src/css/ dist/css/;rsync -a src/fonts/ dist/fonts/;rsync -a node_modules/katex/dist/fonts/*.woff2 dist/fonts/;rsync -a src/js/ dist/js/;mkdirp bin;lsc -bc --no-header -m embedded -p src/cli.ls > bin/latex.js;chmod a+x bin/latex.js;rollup -c;",
    "build": "NODE_ENV=production npm run devbuild;",
    "test": "jest",
    "codecov": "codecov;"
  },
  "dependencies": {
    "commander": "5.x",
    "fs-extra": "9.x",
    "hyphenation.de": "*",
    "hyphenation.en-us": "*",
    "js-beautify": "1.13.x",
    "stdin": "*",
    "svgdom": "^0.1.8"
  },
  "devDependencies": {
    "@types/jest-image-snapshot": "^4.3.1",
    "@types/jsdom-screenshot": "^3.2.1",
    "@rollup/plugin-commonjs": "17.1.x",
    "@rollup/plugin-dynamic-import-vars": "^1.3.0",
    "@rollup/plugin-node-resolve": "11.1.x",
    "@rollup/plugin-typescript": "^8.2.3",
    "@svgdotjs/svg.js": "3.x",
    "@types/he": "^1.1.1",
    "@types/jest": "^26.0.24",
    "@types/js-beautify": "^1.13.2",
    "@types/katex": "^0.11.1",
    "@types/lodash": "^4.14.170",
    "codecov": "3.x",
    "codemirror": "5.59.x",
    "decache": "4.6.x",
    "glob": "^7.1.4",
    "he": "1.2.x",
    "hypher": "0.x",
    "jest": "^27.0.6",
    "jest-image-snapshot": "^4.5.1",
    "jsdom-screenshot": "^4.0.0",
    "katex": "0.12.0",
    "lodash": "4.x",
    "mkdirp": "1.0.x",
    "mocha": "8.x",
    "mocha-junit-reporter": "2.x",
    "nyc": "15.x",
    "peggy": "^1.2.0",
    "pixelmatch": "5.2.x",
    "puppeteer": "5.5.x",
    "raw-loader": "4.0.x",
    "rimraf": "3.x",
    "rollup": "2.53.x",
    "rollup-plugin-terser": "7.0.x",
    "rollup-plugin-visualizer": "4.2.x",
    "serve-handler": "6.x",
    "slugify": "1.4.x",
    "split-grid": "1.0.x",
    "tmp": "0.x",
    "ts-jest": "^27.0.4",
    "tslib": "^2.3.0",
    "typescript": "^4.3.5",
    "vue-codemirror": "4.0.x",
    "vuepress": "1.8.x"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/rogueai/latex.ts.git"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/rogueai/latex.ts/issues"
  },
  "homepage": "",
  "engines": {
    "node": ">= 14.0"
  }
}
